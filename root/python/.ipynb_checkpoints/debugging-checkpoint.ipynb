   "source": [
    "import pdb\n",
    "\n",
    "def add(num1, num2):\n",
    "    pdb.set_trace()   # pause code here and debug\n",
    "    return num1 + num2\n",
    "\n",
    "add(4, 'fdsa')"
   ]
  },
